// Test negative integers

main {
    let a = -2
    print(a)

    let b = 1 + -2
    print(b)

    let c = -3 + -5
    print(c)

    let d = -10 / -2
    print(d)

    let e = 5 * -3
    print(e)

    let f = -7 % 3
    print(f)

    let g = -5
    g = g + 3
    print(g)

    g = g * -2
    print(g)
}
