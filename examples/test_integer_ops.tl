// Test integer arithmetic operations

main {
    let a = 10
    let b = 3

    let sum = a + b
    let diff = a - b
    let prod = a * b
    let quot = a / b
    let rem = a % b

    print(sum)
    print(diff)
    print(prod)
    print(quot)
    print(rem)

    let prec1 = 2 + 3 * 4
    print(prec1)

    let prec2 = 10 - 6 / 2
    print(prec2)

    let prec3 = (2 + 3) * 4
    print(prec3)

    let prec4 = 10 + 7 % 3
    print(prec4)

    let complex1 = 2 + 3 * 4 - 10 / 2
    print(complex1)

    let complex2 = (10 + 5) * 2 - 20 % 7
    print(complex2)

    let token_count = 1
    token_count = token_count + 1
    print(token_count)

    token_count = token_count + 1
    print(token_count)

    token_count = token_count + 1
    print(token_count)

    let cmp1 = 5 + 3 == 8
    print(cmp1)

    let cmp2 = 10 % 3 == 1
    print(cmp2)

    let cmp3 = 2 * 5 > 9
    print(cmp3)
}
