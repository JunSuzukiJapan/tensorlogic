// Test integer calculations

main {
    let a = 10
    let b = 3
    let sum = a + b
    print(sum)

    let diff = a - b
    print(diff)

    let prod = a * b
    print(prod)

    let quot = a / b
    print(quot)

    let rem = a % b
    print(rem)

    let prec1 = 2 + 3 * 4
    print(prec1)

    let prec2 = (2 + 3) * 4
    print(prec2)

    let prec3 = 10 + 7 % 3
    print(prec3)

    let token_count = 1
    token_count = token_count + 1
    print(token_count)
    token_count = token_count + 1
    print(token_count)
}
