{
  "permissions": {
    "allow": [
      "Bash(git push)",
      "Bash(git commit:*)",
      "Bash(cargo test:*)",
      "Bash(for file in examples/*.tl)",
      "Bash(do echo \"=== Checking $file ===\")",
      "Bash(timeout 5 ./target/release/tl run:*)",
      "Bash(done)",
      "Bash(./target/release/tl run:*)",
      "Bash(timeout 60 ./target/release/tl run:*)",
      "Bash(cargo build:*)",
      "Bash(TL_DEBUG_F16_EMBEDDING=1 TL_DEBUG_MMAP=1 timeout:*)",
      "Bash(timeout 120 ./target/release/tl run:*)",
      "Bash(git add:*)",
      "Bash(timeout 30 ./target/release/tl run:*)",
      "Bash(TL_DEBUG_SAMPLING=1 timeout 30 ./target/release/tl run:*)",
      "Bash(TL_DEBUG_SAMPLING=1 timeout 60 ./target/release/tl run:*)",
      "Bash(TL_DEBUG_ROPE=1 TL_DEBUG_SAMPLING=1 timeout 60 ./target/release/tl run:*)",
      "Bash(TL_DEBUG_ACTIVATION=1 timeout 120 ./target/release/tl run:*)",
      "Bash(TL_DEBUG_ACTIVATION=1 timeout 60 ./target/release/tl run:*)",
      "Bash(TL_DEBUG_SAMPLING=1 timeout 120 ./target/release/tl run:*)",
      "Bash(TL_DEBUG_ROPE=1 timeout 60 ./target/release/tl run:*)"
    ],
    "deny": [],
    "ask": []
  }
}
